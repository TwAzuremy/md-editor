{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@renderer/*": [
                "src/renderer/src/*"
            ],
            "@assets/*": [
                "src/renderer/src/assets/*"
            ],
            "@components/*": [
                "src/renderer/src/components/*"
            ],
            "@resources/*": [
                "resources/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@store/*": [
                "src/store/*"
            ]
        }
    }
}